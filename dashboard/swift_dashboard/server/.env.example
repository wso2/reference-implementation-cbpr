HOST=localhost
PORT=9200
OPENSEARCH_INDEX_NAME_MESSAGE=translated_log
OPENSEARCH_INDEX_NAME_LOG=ballerina_log
REACT_APP_API_URL=/api/swift-dashboard
USE_OPENSEARCH=false                                    # Set to true to enable OpenSearch

# Moesif configurable env variables
MOESIF_BASE_URL=https://api.moesif.com
MOESIF_SEARCH_PATH=/v1/search/~/search/events
MOESIF_TRANSLATION_ACTION_NAME=translation_log
MOESIF_LOG_ACTION_NAME=ballerina_log
MOESIF_TIMEOUT=30000                                    # 30 seconds
MOESIF_MAX_RETRIES=3                                    # Number of retry attempts
MOESIF_RETRY_DELAY=1000                                 # Delay between retries in milliseconds
MOESIF_DEBUG=false                                      # Enable debug logging (true/false)
MOESIF_API_KEY=YOUR_MOESIF_API_KEY_HERE                 # Replace with your Moesif API Key
